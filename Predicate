#include <iostream>
#include <algorithm>


using namespace std;

bool gt15(int x)
{
    return 15 < x;
}
int main()
{
  int a[] = {5, 8, 5, 10, 20, 30};
  const int SIZE = sizeof a / sizeof a[0];//Намираме броя на елементите на масива

  int b[SIZE];
  int* endb = remove_copy_if(a, a + SIZE, b, gt15);
  int* beginb = b;

  while(beginb != endb)
  {
      cout << *beginb++ << endl;
  }

  return 0;
}
